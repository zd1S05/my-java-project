<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.app.csv.mapper.ItemCsvExportMapper">
	
	<select id="selectByListingDate" resultType="ItemForCsv">
		SELECT  * FROM items
		WHERE listing_date = #{listingDate}
	</select>
</mapper>